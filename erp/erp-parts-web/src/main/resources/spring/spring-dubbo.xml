<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd">

    <!-- 自定义服务名称 -->
    <dubbo:application name="ErpPartsWeb"/>

    <!-- 服务注册中心的地址和端口 -->
    <dubbo:registry address="zookeeper:192.168.125:2181"/>

    <!-- 接受服务 -->
    <dubbo:reference interface="com.kaisheng.it.service.EmployeeService" id="employeeService"/>
    <dubbo:reference interface="com.kaisheng.it.service.PartsService" id="partsService"/>
    
</beans>